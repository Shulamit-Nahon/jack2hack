#!agena

run '../lib/input.agn'
run 'parse_args.agn'
run 'asm_gen.agn'
run 'parser.agn'

# TODO remove this if. its for testing only
if args[1] = null then
	args[1] := 'test.vm'
fi

fin_name, fout_name := parse_args('asm')
asm_gen@@init(fout_name)

for keys line in io.lines(fin_name) do
	parser@@parse_line(line)
od